{
  "name": "linqify",
  "version": "1.1.1",
  "description": "Javascript LINQ library based on C# LINQ",
  "main": "dist/linqify.js",
  "module": "dist/linqify.esm.js",
  "unpkg": "dist/linqify.min.js",
  "browser": "dist/linqify.min.js",
  "types": "dist/linqify.d.ts",
  "scripts": {
    "doc": "typedoc --out doc/ types/linqify.ts --target ES6 --media media/ --module commonjs --name \"LINQify Documentation\" --theme default --mode file",
    "prettier": "prettier --write \"src/**/*.js\"",
    "eslint": "eslint \"src/**/*.js\" --fix",
    "test": "set LINQIFY_PATH=/src/linqify&&jest --coverage --colors",
    "build": "rollup -c rollup.config.js&&npm run doc&&cp types/linqify.ts dist/linqify.d.ts&&cp types/linqify.ts src/linqify.d.ts",
    "build:test": "npm run build&&set LINQIFY_PATH=/dist/linqify&&jest&&set LINQIFY_PATH=/dist/linqify.min&&jest",

    "pregitpagespublish": "npm run doc",
    "gitpagespublish": "cd doc&&git init&&git add --all&&git commit -m \"Doc commit\"&&git push -f -u https://github.com/linqify/linqify.github.io.git master",

    "gitbuild": "npm run test&&npm run prettier&&npm run eslint&&npm run build:test",
    "gitpublish": "git push -f -u https://github.com/goranhrovat/linqify.git master",
            
    "predeploy": "npm run gitbuild",
    "deploy": "npm login&&npm publish"
  },
  "keywords": [
    "lazy",
    "LINQ",
    "IEnumerable",
    "Dictionary",
    "HashSet",
    "List",
    "Lookup"
  ],
  "license": "MIT",
  "author": "Goran Hrovat",
  "repository": {
    "type": "git",
    "url": "git://github.com/goranhrovat/linqify.git"
  },
  "homepage": "https://linqify.github.io",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/runtime": "^7.2.0",
    "coveralls": "^3.0.2",
    "eslint": "^5.13.0",
    "eslint-config-prettier": "^4.0.0",
    "jest": "^24.1.0",
    "prettier": "^1.16.4",
    "rollup": "^1.1.2",
    "rollup-plugin-babel": "^4.2.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-terser": "^4.0.4",
    "typedoc": "^0.14.2"
  }
}
